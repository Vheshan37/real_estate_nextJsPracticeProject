generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model status {
  id     Int    @id @default(autoincrement())
  status String @db.VarChar(45)
  user   user[]
}

model user {
  id        Int    @id @default(autoincrement())
  email     String @db.VarChar(50)
  name      String @db.VarChar(100)
  password  String @db.VarChar(255)
  status_id Int
  status    status @relation(fields: [status_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_user_status")

  @@index([status_id], map: "fk_user_status_idx")
}
